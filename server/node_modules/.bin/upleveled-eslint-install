#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/larsreuter/projects/gigster-app/server/node_modules/.pnpm/eslint-config-upleveled@7.8.1_@babel+core@7.24.0_@types+eslint@8.56.5_@types+node@20.11.25_@t_6vpi556kmiofyvb2gfpf4az64y/node_modules/eslint-config-upleveled/bin/node_modules:/Users/larsreuter/projects/gigster-app/server/node_modules/.pnpm/eslint-config-upleveled@7.8.1_@babel+core@7.24.0_@types+eslint@8.56.5_@types+node@20.11.25_@t_6vpi556kmiofyvb2gfpf4az64y/node_modules/eslint-config-upleveled/node_modules:/Users/larsreuter/projects/gigster-app/server/node_modules/.pnpm/eslint-config-upleveled@7.8.1_@babel+core@7.24.0_@types+eslint@8.56.5_@types+node@20.11.25_@t_6vpi556kmiofyvb2gfpf4az64y/node_modules:/Users/larsreuter/projects/gigster-app/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/larsreuter/projects/gigster-app/server/node_modules/.pnpm/eslint-config-upleveled@7.8.1_@babel+core@7.24.0_@types+eslint@8.56.5_@types+node@20.11.25_@t_6vpi556kmiofyvb2gfpf4az64y/node_modules/eslint-config-upleveled/bin/node_modules:/Users/larsreuter/projects/gigster-app/server/node_modules/.pnpm/eslint-config-upleveled@7.8.1_@babel+core@7.24.0_@types+eslint@8.56.5_@types+node@20.11.25_@t_6vpi556kmiofyvb2gfpf4az64y/node_modules/eslint-config-upleveled/node_modules:/Users/larsreuter/projects/gigster-app/server/node_modules/.pnpm/eslint-config-upleveled@7.8.1_@babel+core@7.24.0_@types+eslint@8.56.5_@types+node@20.11.25_@t_6vpi556kmiofyvb2gfpf4az64y/node_modules:/Users/larsreuter/projects/gigster-app/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint-config-upleveled/bin/install.js" "$@"
else
  exec node  "$basedir/../eslint-config-upleveled/bin/install.js" "$@"
fi
