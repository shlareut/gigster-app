#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/larsreuter/projects/gigster-app/next-server/node_modules/.pnpm/ley@0.8.1/node_modules/ley/node_modules:/Users/larsreuter/projects/gigster-app/next-server/node_modules/.pnpm/ley@0.8.1/node_modules:/Users/larsreuter/projects/gigster-app/next-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/larsreuter/projects/gigster-app/next-server/node_modules/.pnpm/ley@0.8.1/node_modules/ley/node_modules:/Users/larsreuter/projects/gigster-app/next-server/node_modules/.pnpm/ley@0.8.1/node_modules:/Users/larsreuter/projects/gigster-app/next-server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
